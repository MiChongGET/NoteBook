<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.notebook">

    <application
        android:name=".Passwd.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainUI.MainActivity" />
        <activity android:name=".Guide.Guide" />
        <activity
            android:name=".Guide.WelcomeAct"
            android:theme="@style/welcompage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Passwd.PassWordActivity" />
        <activity android:name=".Setting.Setting" />
        <activity android:name=".Functions.Write_note" />
        <activity
            android:name=".Functions.Read_note"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            />
    </application>

</manifest>